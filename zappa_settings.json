{
    "dev": {
        "app_function": "app.f",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "dollar",
        "runtime": "python3.10",
        "s3_bucket": "dollar-mm",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "environment_variables": {
        "RDS_HOST": "${{ secrets.RDS_HOST }}",
        "RDS_USER": "${{ secrets.RDS_USER }}",
        "RDS_PASSWORD": "${{ secrets.RDS_PASSWORD }}",
        "RDS_DB": "${{ secrets.RDS_DB }}"
    },
        "role_name": "LabRole",
        "events": [
            {
                "function": "app.f",
                "expression": "cron(41 5 * * ? *)"
            },
            {
                "function": "app.process_file",
                "event_source": {
                    "arn": "arn:aws:s3:::dolar-raw-cmjm",
                    "events": ["s3:ObjectCreated:*"]
                }
            }
        ]
    }
}