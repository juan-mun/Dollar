{
    "fetch": {
        "app_function": "app.f",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "dollar-fetch",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-cmjm",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "app.f",
                "expression": "cron(2 1 * * ? *)"
            }
        ]
    },
    "process": {
        "app_function": "app.process_file",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "dollar-process",
        "runtime": "python3.10",
        "s3_bucket": "dolar-raw-cmjm-deploy",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "environment_variables": {
            "RDS_HOST": "${RDS_HOST}",
            "RDS_USER": "${RDS_USER}",
            "RDS_PASSWORD": "${RDS_PASSWORD}",
            "RDS_DB": "${RDS_DB}"
        },
        "events": [
            {
                "function": "app.process_file",
                "event_source": {
                    "arn": "arn:aws:s3:::dolar-raw-cmjm",
                    "events": ["s3:ObjectCreated:*"],
                    "filters": [
                        {
                            "name": "prefix",
                            "value": "dolar-"
                        },
                        {
                            "name": "suffix", 
                            "value": ".json"
                        }
                    ]
                }
            }
        ]
    }
}